CREATE TABLE strload_tables (
  id integer NOT NULL
  , source_id character varying(256) UNIQUE NOT NULL
  , schema_name character varying(128) NOT NULL
  , table_name character varying(128) NOT NULL
  , load_batch_size integer NOT NULL
  , load_interval integer NOT NULL
  , load_option jsonb
  , disabled boolean not null
  , CONSTRAINT strload_tables_pkey PRIMARY KEY (id)
  , CONSTRAINT strlaod_tables_uniq UNIQUE (schema_name, table_name)
);
