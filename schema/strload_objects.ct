CREATE TABLE strload_objects (
  object_seq bigserial NOT NULL
  , source_id character varying(256)  NOT NULL
  , object_url character varying(512) NOT NULL
  , object_size integer NOT NULL
  , submit_time timestamp with time zone NOT NULL
  , CONSTRAINT strload_objects_pkey PRIMARY KEY (object_seq)
);
CREATE UNIQUE INDEX strload_objects_object_url ON strload_objects (object_url);
